{
  "name": "ccusage-slack",
  "version": "1.0.0",
  "description": "Update Slack profile with Claude usage cost savings",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "build": "pkg . --out-path build"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "sharp": "^0.34.2"
  },
  "keywords": [
    "slack",
    "claude",
    "usage",
    "profile"
  ],
  "author": "SanMurakami",
  "license": "MIT",
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "build",
    "assets": [
      "node_modules/axios/**/*"
    ],
    "scripts": [
      "node_modules/axios/dist/node/axios.cjs"
    ]
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
